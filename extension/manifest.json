{
  "manifest_version": 3,
  "name": "AutoFill Extension",
  "description": "Automatically fill forms on web pages.",
  "version": "1.0",
  "icons": {
    "16": "icons/aiApply.png",
    "32": "icons/aiapply.png",
    "48": "icons/aiapply.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "nativeMessaging"
  ],
  "action": {
    "default_icon": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    },
    "default_popup": "./popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}